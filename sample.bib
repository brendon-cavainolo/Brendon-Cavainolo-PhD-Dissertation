@Article{Kabir,
AUTHOR = {Kabir, Mohammad Rizviul and Sirigiri, Anil Kumar and Naraparaju, Ravisankar and Schulz, Uwe},
TITLE = {Flow Kinetics of Molten Silicates through Thermal Barrier Coating: A Numerical Study},
