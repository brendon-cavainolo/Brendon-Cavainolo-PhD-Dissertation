@inbook{Bielawski2024,
author = {Ral Bielawski and Venkatramanan Raman},
title = {Analysis of Secondary Droplet Formation by Shock Induced Break-up of Droplets},
booktitle = {AIAA SCITECH 2024 Forum},
chapter = {},
pages = {},
doi = {10.2514/6.2024-1639},
URL = {https://arc.aiaa.org/doi/abs/10.2514/6.2024-1639},
eprint = {https://arc.aiaa.org/doi/pdf/10.2514/6.2024-1639},
    abstract = { Shock droplet interactions are found in a wide range of applications, ranging from liquid- fueled detonation engines to droplet interactions with high-speed vehicles and detonation mitigation. However, the physical understanding and modeling of such multiphase detonations are hampered by a lack of models for droplet break after a shock or detonation. Experimental studies of this problem are available but can only measure macroscopic properties and cannot resolve the quantitative details at small scales. High-fidelity numerical studies are needed to understand the break-up process better. The current study will use fully-resolved simulations of Mach 3, 4, and 5 shock waves interacting with a 100 micro meter water droplet. The post-impact primary droplet and the secondary shed droplet radius are computed and found to have a log-normal distribution. To the author’s knowledge, this is the first high-fidelity study of shock-droplet interactions that captures the shed-droplet distribution. }
}

