eprint = {https://analyticalsciencejournals.onlinelibrary.wiley.com/doi/pdf/10.1002/elsc.200390042},
abstract = {As has been confirmed by several studies reported in the literature, cavitation phenomena considerably contribute to droplet breakup during emulsification in high-pressure homogenizers. High-pressure homogenizers can be equipped with different homogenizing valves. The question, whether cavitation is also of importance for droplet breakup in orifice valves which have been proved very efficient for some emulsion systems, has not been investigated yet and is the subject of this contribution.},
