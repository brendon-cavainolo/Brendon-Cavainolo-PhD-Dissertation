@article{CAUPIN20061000,
title = {Cavitation in water: a review},
journal = {Comptes Rendus Physique},
volume = {7},
number = {9},
pages = {1000-1017},
year = {2006},
note = {Nucleation},
issn = {1631-0705},
doi = {https://doi.org/10.1016/j.crhy.2006.10.015},
url = {https://www.sciencedirect.com/science/article/pii/S1631070506002283},
author = {Frédéric Caupin and Eric Herbert},
keywords = {Water, Metastability, Superheated liquid, Stretched liquid, Cavitation, Eau, Métastabilité, Liquide surchauffé, Liquide sous tension, Cavitation},
abstract = {Liquid water can be brought beyond the liquid–vapor equilibrium line into a metastable state, before nucleation of bubbles (cavitation) occurs. We review the experimental work on cavitation in water, focusing on the determination of the ultimate degree of metastability at which liquid water can exist. We also present practical applications of metastability and cavitation. To cite this article: F. Caupin, E. Herbert, C. R. Physique 7 (2006).
Résumé
L'eau liquide peut être amenée au-delà de la ligne d'équilibre liquide–vapeur jusque dans un état métastable, avant que la nucléation de bulles (cavitation) n'intervienne. Nous passons en revue les travaux expérimentaux sur la cavitation dans l'eau, en mettant l'accent sur la détermination du degré de métastabilité extrême auquel l'eau liquide peut exister. Nous présentons également des applications pratiques de la métastabilité et de la cavitation. Pour citer cet article : F. Caupin, E. Herbert, C. R. Physique 7 (2006).}
}
